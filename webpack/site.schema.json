{
  "type": "object",
  "required": [],
  "properties": {
    "basePath": {
      "type": "string",
      "default": "/",
      "pattern": "^\\/[-a-zA-Z0-9]*?$"
    },
    "internalDomains": {
      "type": "array",
      "default": [],
      "items": { "type": "string", "pattern": "^[-.:a-zA-Z0-9]+$" }
    },
    "titlePostfix": { "type": "string" },
    "courseId": { "type": "string" },
    "courseCode": { "type": "string" },
    "courseTitle": { "type": "string" },
    "fullCourseName": { "type": "string" },
    "faviconColor": {
      "type": "string",
      "pattern": "^hsl\\(\\d+(deg)? \\d+% \\d+%\\)$"
    },
    "faviconSymbol": { "type": "string", "pattern": "^[A-Z0-9\\-]{1,4}$" }
  },
  "additionalProperties": false
}
